static vector<bool, N> equal_to(const self& a, const self& b) {
	return vector<bool, N>::make_from_simd(a.to_simd() == b.to_simd());
}
static vector<bool, N> not_equal_to(const self& a, const self& b) {
	return vector<bool, N>::make_from_simd(a.to_simd() != b.to_simd());
}

static vector<bool, N> less_than(const self& a, const self& b) {
	return vector<bool, N>::make_from_simd(a.to_simd() < b.to_simd());
}
static vector<bool, N> less_than_or_equal_to(const self& a, const self& b) {
	return vector<bool, N>::make_from_simd(a.to_simd() <= b.to_simd());
}
static vector<bool, N> greater_than(const self& a, const self& b) {
	return vector<bool, N>::make_from_simd(a.to_simd() > b.to_simd());
}
static vector<bool, N> greater_than_or_equal_to(const self& a, const self& b) {
